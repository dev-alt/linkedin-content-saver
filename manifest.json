{
    "manifest_version": 2,
    "name": "LinkedIn Saver",
    "version": "1.0.0",
    "description": "LinkedIn page saver, image, and PDF grabber",
    "author": "Andrew",
    "icons": {
      "48": "icons/icon48.png",
      "96": "icons/icon96.png"
    },
    "permissions": [
      "tabs",
      "downloads",
      "storage"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://www.linkedin.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  